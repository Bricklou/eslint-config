  "repository": {
    "type": "git",
    "url": "https://github.com/Bricklou/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/Bricklou/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nr gen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsdown --watch",
    "lint": "eslint",
    "gen": "tsx scripts/typegen.ts && tsx scripts/versiongen.ts",
    "prepack": "nr build",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@angular-eslint/eslint-plugin": "catalog:peer",
